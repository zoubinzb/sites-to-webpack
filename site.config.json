{
     "countrys": {
          "us": {
               "device": [
                    "pc",
                    "ipad"
               ],
               "build_type": "default"
          }
     },
     "siteRootDirectory": {
          "rootWaitCopyFile": [
               "ads.txt",
               "robots.txt"
          ],
          "rootCommonPath": "./template/common/"
     },
     "entry": {
          "pc-index": "./template/pages/pc/index/index.js",
          "commonCss": "./template/static/css/common.css"
     },
     "indexPage": {
          "default": {
               "template": [
                    "./template/pages/$2/index/index.pug",
                    "./template/pages/privacy/index.pug",
                    "./template/pages/contacts/index.pug",
                    "./template/pages/about/index.pug"
               ],
               "filename": [
                    "$/$1/$2/index.html",
                    "$/$1/$2/privacy.html",
                    "$/$1/$2/contacts.html",
                    "$/$1/$2/about.html"
               ],
               "data": [
                    "indexData"
               ],
               "options": [
                    0,
                    0,
                    0,
                    0
               ]
          }
     },
     "common": "$common",
     "dev_config": {
          "name": "puzzleABC",
          "dev_siteTarget": "us",
          "device": "pc"
     }
}